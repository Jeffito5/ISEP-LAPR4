    class "<VO> Autonomy"
    class "<VO> Address"
    class "<VO> Weight"
    class "<VO> Volume"
    class "<VO>Warehouse Location"
    class "<Entity> Dock"
    {
        id
        xInicio
        xFim
        yInicio
        yFim
    }
}




